{
  "expo": {
    "name": "desafio-BeMobile",
    "slug": "desafio-BeMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/beMobile-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/beMobile-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1C1C1C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/beMobile-adaptive-icon.png",
        "backgroundColor": "#1C1C1C"
      }
    }
  }
}
